package binary_search_problems;

public class First_And_Last_Occurence_Of_Num {
	
	public static void main(String[] args) {
		
		 int arr[]= {2,4,6,8,8,8,11,13};
		 int first=-1;
		 int last=-1;
		 int x=8;
		 
		 for(int i=0;i<=arr.length-1;i++) {
			 if(arr[i]==x) {
				 first=arr[i];
			 }
			 
		 }
	}

}
